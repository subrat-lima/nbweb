{
    "https://odishatv.in": {
        "name": "OdishaTV",
        "rules": {
            "title": {
                "selector": "h1::text"
            },
            "img": {
                "selector": "div#storymultiimg div.article-featured > img::attr(data-src)"
            },
            "content": {
                "selector": "div.article-content > p::text"
            }
        }
    },
    "https://www.techspot.com" : {
        "name": "TechSpot",
        "rules": {
            "title": {
                "selector": "h1::text"
            },
            "content": {
                "selector": "div.articleBody > :not(div.related-news)::text"
            }
        }
    }
}
